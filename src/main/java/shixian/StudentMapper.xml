<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"      
 "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="shixian.smapper">
<!-- 开启二级缓存 -->
	<cache/>
	
	<select id="findk" parameterType="string" resultType="s">
	
	select * from qqq where id=#{id}
	</select>
	
	
	
	
	<delete id="intk" parameterType="int">
		delete from qqq where id=#{id}
	</delete>
</mapper>